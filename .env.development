# ================================================
# Fuse React ViteJS - Development Environment
# ================================================

# API Configuration - Development
VITE_API_BASE_URL=http://localhost:3000

# Google Map Configurations
VITE_MAP_KEY=your_development_google_map_api_key

# Firebase Auth Configurations - Development
VITE_FIREBASE_API_KEY=YOUR_DEV_API_KEY
VITE_FIREBASE_AUTH_DOMAIN=your-dev-app.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your-dev-app.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your-dev-app
VITE_FIREBASE_STORAGE_BUCKET=your-dev-app.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=YOUR_DEV_MESSAGING_SENDER_ID
VITE_FIREBASE_APP_ID=YOUR_DEV_APP_ID

# AWS Cognito Configurations - Development
VITE_AWS_PROJECT_REGION=us-east-1
VITE_AWS_COGNITO_IDENTITY_POOL_ID=your-dev-identity-pool-id
VITE_AWS_COGNITO_REGION=us-east-1
VITE_AWS_USER_POOLS_ID=your-dev-user-pool-id
VITE_AWS_USER_POOLS_WEB_CLIENT_ID=your-dev-web-client-id
VITE_AWS_OAUTH_DOMAIN=your-dev-domain.auth.us-east-1.amazoncognito.com
VITE_AWS_OAUTH_REDIRECT_SIGN_IN=http://localhost:3000
VITE_AWS_OAUTH_REDIRECT_SIGN_OUT=http://localhost:3000

# App Configuration - Development
VITE_APP_ENV=development
VITE_ENABLE_MOCK_API=true
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Security - Development
VITE_ENABLE_CSP=false
VITE_ALLOWED_HOSTS=localhost,127.0.0.1

# Performance - Development
VITE_ENABLE_COMPRESSION=false
VITE_ENABLE_CACHE=false
VITE_CACHE_DURATION=0

# Development Tools
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_ENABLE_REACT_QUERY_DEVTOOLS=true
VITE_ENABLE_HOT_RELOAD=true

# Build Configuration - Development
VITE_BUNDLE_ANALYZER=false
VITE_BUILD_TARGET=esnext

# Feature Flags - Development
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_RTL=true
VITE_ENABLE_MULTI_LANGUAGE=true

# Mock API Settings
VITE_MOCK_DELAY=500
VITE_MOCK_ERROR_RATE=0

# Keycloak Configurations - Development
VITE_KEYCLOAK_URL=https://dev-ppsso.polytronev.id  
VITE_KEYCLOAK_REALM=pmcp  
VITE_KEYCLOAK_CLIENT=fe-status-container
VITE_KEYCLOAK_REFRESH=1800

# App Base URL for Keycloak redirects
VITE_APP_BASE_URL=https://d6xvsm6t-3000.asse.devtunnels.ms

# Enable Mock ? (set to false when using Keycloak)
VITE_ENABLE_MSW=false
VITE_ENABLE_TRAILMOCK=false
