# ================================================
# Fuse React ViteJS - Production Environment
# ================================================

# API Configuration - Production
VITE_API_BASE_URL=https://your-production-api.com

# Google Map Configurations
VITE_MAP_KEY=your_production_google_map_api_key

# Firebase Auth Configurations - Production
VITE_FIREBASE_API_KEY=YOUR_PRODUCTION_API_KEY
VITE_FIREBASE_AUTH_DOMAIN=your-prod-app.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your-prod-app.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your-prod-app
VITE_FIREBASE_STORAGE_BUCKET=your-prod-app.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=YOUR_PRODUCTION_MESSAGING_SENDER_ID
VITE_FIREBASE_APP_ID=YOUR_PRODUCTION_APP_ID

# AWS Cognito Configurations - Production
VITE_AWS_PROJECT_REGION=us-east-1
VITE_AWS_COGNITO_IDENTITY_POOL_ID=your-production-identity-pool-id
VITE_AWS_COGNITO_REGION=us-east-1
VITE_AWS_USER_POOLS_ID=your-production-user-pool-id
VITE_AWS_USER_POOLS_WEB_CLIENT_ID=your-production-web-client-id
VITE_AWS_OAUTH_DOMAIN=your-production-domain.auth.us-east-1.amazoncognito.com
VITE_AWS_OAUTH_REDIRECT_SIGN_IN=https://your-production-domain.com
VITE_AWS_OAUTH_REDIRECT_SIGN_OUT=https://your-production-domain.com

# App Configuration
VITE_APP_ENV=production
VITE_ENABLE_MOCK_API=false
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# Security - Production
VITE_ENABLE_CSP=true
VITE_ALLOWED_HOSTS=your-production-domain.com

# Performance - Production
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHE=true
VITE_CACHE_DURATION=86400000

# Analytics - Production
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_SENTRY_DSN=https://xxxxxxxxx@sentry.io/xxxxxxx

# Build Configuration - Production
VITE_BUNDLE_ANALYZER=true
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_ENABLE_REACT_QUERY_DEVTOOLS=false
